#Question1
select data1.order_id,data1.product_id from data1
left join data2
on data1.order_id = data2.order_id
where data2 is null

#for no. of missing order_id and product_id 
select count(*) from data1
left join data2
on data1.order_id = data2.order_id
where data2 is null
answer=129

